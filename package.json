{
  "name": "DeltaPrime",
  "version": "0.1.0",
  "description": "Smart loans",
  "author": "Jakub Wojciechowski <jakub@alice.si>",
  "scripts": {
    "dev": "node scripts/dev-server.js",
    "start": "yarn migrate:local && yarn dev",
    "start:server": "yarn migrate:server && vercel --prod",
    "build:contracts:hardhat": "yarn hardhat compile",
    "build:contracts:hardhat:force": "yarn hardhat compile --force",
    "build:contracts:truffle": "ruffle compile",
    "build:contracts:truffle:force": "rm -rf build && truffle compile",
    "build": "yarn build:contracts:truffle && node scripts/build.js",
    "test-local": "yarn hardhat test test/local/*",
    "test-local:deprecated": "yarn hardhat test test/deprecated/*/*",
    "test-integration": "yarn hardhat test test/integration/* --network hardhat-integration",
    "forked-test-node": "yarn hardhat node --fork https://api.avax.network/ext/bc/C/rpc",
    "migrate:local": "npx truffle migrate --network local",
    "migrate:server": "npx truffle migrate --network server",
    "test:watch": "yarn hardhat test && yarn hardhat watch test",
    "ci:watch": "yarn hardhat watch ci",
    "prettier": "prettier --write 'contracts/**/*.sol'"
  },
  "dependencies": {
    "@growthbunker/vuetimeline": "^0.1.17",
    "@openzeppelin/contracts": "^4.1.0",
    "@openzeppelin/contracts-upgradeable": "^4.2.0",
    "@pangolindex/exchange-contracts": "github:pangolindex/exchange-contracts",
    "@uniswap/lib": "^4.0.1-alpha",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "chart.js": "^2.9.4",
    "countup.js": "^2.0.7",
    "erc-1155": "git+https://github.com/enjin/erc-1155.git",
    "ethers": "^5.4.0",
    "javascript-time-ago": "^2.3.9",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "redstone-api": "^0.4.6",
    "redstone-evm-connector": "^1.3.5",
    "solidity-docgen": "^0.3.13",
    "truffle": "^5.4.3",
    "truffle-contract": "^4.0.31",
    "v-tooltip": "^2.1.3",
    "vue": "^2.6.14",
    "vue-async-computed": "^3.9.0",
    "vue-chartjs": "^3.5.1",
    "vue-countup-v2": "^4.0.0",
    "vue-loaders": "^4.1.4",
    "vue-router": "^2.8.1",
    "vue-toastification": "^1.7.11",
    "vue2-filters": "^0.13.0",
    "vuelidate": "^0.7.6",
    "vuex": "^3.6.2",
    "web3-detect-network": "0.0.18"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-truffle5": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/test-helpers": "^0.5.11",
    "@truffle/hdwallet-provider": "^1.3.1",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.1.0",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.3",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "buidler": "^0.2.8",
    "chai": "^4.3.4",
    "chromedriver": "^91.0.1",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.28.0",
    "cssnano": "^3.10.0",
    "es6-promisify": "6.0.2",
    "ethereum-waffle": "^3.4.0",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "hardhat": "^2.4.1",
    "hardhat-watcher": "^2.1.1",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.3",
    "inject-loader": "^3.0.0",
    "node-sass": "^6.0.1",
    "opn": "^6.0.0",
    "optimize-css-assets-webpack-plugin": "^2.0.0",
    "ora": "^5.4.1",
    "push-dir": "^0.4.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^6.0.6",
    "shelljs": "^0.7.6",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.0.0",
    "typechain": "^5.1.1",
    "typescript": "^4.3.4",
    "url-loader": "^0.6.2",
    "verify-on-etherscan": "^1.2.1",
    "vue-loader": "^14.2.4",
    "vue-style-loader": "^3.1.2",
    "vue-template-compiler": "^2.6.14",
    "web3": "1.2.9",
    "webpack": "^2.7.0",
    "webpack-bundle-analyzer": "4.4.2",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  },
  "engines": {
    "node": ">= 4.0.0 <= 16.13.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false,
          "targets": {
            "browsers": [
              "> 1%",
              "last 2 versions",
              "not ie <= 8"
            ]
          }
        }
      ],
      "stage-2"
    ],
    "plugins": [
      "transform-runtime",
      "transform-object-rest-spread"
    ],
    "env": {}
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.sol",
        "options": {
          "tabWidth": 2
        }
      }
    ]
  }
}
